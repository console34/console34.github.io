---
const logos = [
	{src:"/images/logos/nets-logo.png",
		alt:"nets",
	},
	{
		src: "l/images/logos/ogo-iuss.svg",
		alt: "iuss",
	},
	{
		src: "/images/logos/efc-logo.webp",
		alt: "efc",
	},
	{
		src: "/images/logos/imt-logo.png",
		alt: "imt",
	},
	
];
---

<div class="mx-auto max-w-7xl px-6 lg:px-8 py-20">
	<div
		class="mx-auto grid max-w-lg grid-cols-4 items-center gap-x-8 gap-y-12 sm:max-w-xl sm:grid-cols-4 sm:gap-x-10 sm:gap-y-14 lg:mx-0 lg:max-w-none lg:grid-cols-4">
		{
			logos.map(({ src, alt }) => (
				<img class="col-span-2 max-h-12 w-full object-contain lg:col-span-1 dark:invert" src={src} alt={alt} width="158" height="48" />
			))
		}
	</div>
</div>
